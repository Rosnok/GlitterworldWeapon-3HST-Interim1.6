  <Gun_3HST_LGIS_Launcher.comps.CompSecondaryAmmo.mainWeaponLabel>直射</Gun_3HST_LGIS_Launcher.comps.CompSecondaryAmmo.mainWeaponLabel>
  <!-- EN: indirect fire -->
  <Gun_3HST_LGIS_Launcher.comps.CompSecondaryAmmo.secondaryWeaponLabel>曲射</Gun_3HST_LGIS_Launcher.comps.CompSecondaryAmmo.secondaryWeaponLabel>
  <!-- EN: Rail durability -->
  <Gun_3HST_LGIS_Launcher.comps.CompChangeBarrel.label>磁轨寿命</Gun_3HST_LGIS_Launcher.comps.CompChangeBarrel.label>
